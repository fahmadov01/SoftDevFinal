<%- include('../partials/navbar.ejs') %>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
</head>
<body>
    <main>
        <div class="container">
          <% if (locals.message) { %>
              <div
                class="alert alert-<%- locals.error ? 'danger' : 'success' %>"
                role="alert"
              >
                <%= locals.message %>
              </div>
              <% } %>
            <table class="table">
              <thead>
                <tr>
                  <th scope="row">URL</th>
                  <th scope="row">Date</th>
                  <th scope="row">Time</th>
                  <th scope="row">Title</th>
                </tr>
              </thead>
      
              <tbody>
                <% results.forEach((events) => {%>
                  <tr>
                    <td><%= events.url %></td>
                     <td> <%= events.date %> </td>
                    <td> <%= events.time %> </td>
                    <td> <%= events.title %> </td>
                  </tr>
                <% }); %>
              </tbody>
            </table>
        </div>
      </main>
</body>
</html>
